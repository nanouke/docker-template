<?xml version="1.0" encoding="utf-8"?>
<Container>
    <Name Default="Plex-vpn">Plex-vpn</Name>
    <Overview>Instance of plex but connected to a VPN tunnel</Overview>
    <Registry>https://hub.docker.com/u/dpage</Registry>
    <Repository>dpage/pgadmin4</Repository>
    <Privileged>false</Privileged>

    <BindTime>true</BindTime>

    <Environment>
        <Variable>
            <Name>PGADMIN_DEFAULT_EMAIL</Name>
            <Value></Value>
        </Variable>
        <Variable>
            <Name>PGADMIN_DEFAULT_PASSWORD</Name>
            <Value></Value>
        </Variable>
        <Variable>
            <Name>PGADMIN_ENABLE_TLS</Name>
            <Value>false</Value>
        </Variable>
        <Variable>
            <Name>PGADMIN_LISTEN_PORT</Name>
            <Value>80</Value>
        </Variable>
        <Variable>
            <Name>GUNICORN_THREADS</Name>
            <Value>25</Value>
        </Variable>
    </Environment>

    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
                <HostPort>5050</HostPort>
                <ContainerPort>80</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>

    <Data>
        <Volume>
            <HostDir>/mnt/user/appdata/pgadmin</HostDir>
            <ContainerDir>/var/lib/pgadmin</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
        <Volume>
            <HostDir>/mnt/user/appdata/pgadmin/certs</HostDir>
            <ContainerDir>/certs/</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
    <WebUI>http://[IP]:[PORT:5050]</WebUI>
    <Icon>https://www.postgresql.org/media/img/about/press/elephant.png</Icon>
    <ExtraParams></ExtraParams>
</Container>